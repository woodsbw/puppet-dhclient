#
# This configuration file is managed by Puppet. Do not make manual changes.
#

option rfc3442-classless-static-routes code 121 = array of unsigned integer 8;

<% scope.lookupvar('dhclient::merged_sends').sort.each_with_index do |(param, value), index| -%>
<% if value -%>
send <%= param -%> = <%= value -%>;

<% end -%>
<% end -%>


request <% scope.lookupvar('dhclient::merged_requests').sort.each_with_index do |(param, value), index| -%>
<% if value -%>
    <%= param -%>
<% if index+1 == scope.lookupvar('dhclient::merged_requests').length -%>;<% else %>,<% end %>
<% end -%>
<% end -%>

